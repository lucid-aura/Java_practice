package ifElse;

public class MainClass {

	public static void main(String[] args) {
		/*
		 * 	if : 조건문
		 * 	형식
		 * 	if ( 조건 ) { 조건 -> true/false
		 * 		처리
		 * 	}
		 * 
		 * 	조건 : ==(같다), >(크다), <(작다), >=(크거나 같다), <=(작거나 같다), !=(같지 않다) 
		 * 
		 * 	다수의 조건 : 논리 연산자와 같이 사용 &&(AND), ||(OR), !(NOT)
		 * 
		 * 	if ( 조건1 && 조건2 ) {
		 * 
		 * 	}
		 */
		
		int number = 5;
		
		if (number > 0) {
			System.out.println("zz");
		}
	}

}
