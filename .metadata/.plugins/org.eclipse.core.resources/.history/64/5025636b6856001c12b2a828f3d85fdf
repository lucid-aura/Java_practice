		System.out.println("========================================");
		System.out.println("\\ name\t\tage\tphone\t\t\taddress\t\\");
		System.out.println("========================================");
		System.out.println("\\ "+ name1 + "\t" + age1 + "\t" + phone1 + "\t" + height1 + "\t" + address");
		System.out.println("\\ \"일지매\"\t18\t02-123-4567\t\'서울\'\t\\");
