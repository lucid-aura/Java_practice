package day5;

public class Method {

	public static void main(String[] args) {
		/*
		 * function : 함수. 독립적
		 * method : class에 소속된 함수.
		 * 
		 *	형식:
		 *	
		 *	돌아오는 값의 자료형 함수명(들어가는 값의 자료형 변수, 들어가는 값의 자료형 변수, ..._){
		 *			처리
		 *			return 값;
		 *	}
		 *
		 *	void -> 자료형이 없는 것
		 */
		
		functionName('A');
		
				
		
	}
	
	static int functionName(char c) {
		System.out.println("functionName(char c) 호출");
		
		return 1;
	}

}
