package forLoop;

public class MainClass {

	public static void main(String[] args) {
		int n;
		for (n=0; n<2; n++) {
			System.out.println("for loop" + n);
		}
		
		for (int i=0; i<10; i++) {
			System.out.print(i + " ");
		}
		System.out.println();
		
		for (int i=0; i<20; i++) {
			System.out.print(i + " ");
		}
	}

}
