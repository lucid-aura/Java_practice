package day6;

import java.io.File;

public class FileIO {

	public static void main(String[] args) {
		/*
		 * File : 저장 매체  / Database(file)
		 */
		
		File file = new File("c:\\");
		//  파일 조사
		
		String filelist[] = file.list();
		for (int i = 0; i < filelist.length; i++) {
			System.out.println(filelist[i]);
		}
	}

}
