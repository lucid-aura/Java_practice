package whileLoop;

public class MainClass {

	public static void main(String[] args) {
		/*
		 * 초기화
		 * while(조건식) {
		 * 	처리
		 * 	연산식
		 * }
		 * 
		 * 초기화
		 * do {
		 * 	처리
		 * 	연산식
		 * }while(조건식);
		 */
		
		int w;
		w = 0;
		while( w < 10 ) {
			System.out.println("while loop" + w);
			w++;
		}
		
		int dw;
		dw = 0;
		do {
			System.out.println("do while loop" + dw);
			dw++;
		}while( dw < 10 );
		
		int i = 1;
		int j = 1;
		while(i < 10) {
			while (j < 10) {
				System.out.print(i + "x" + j + "=" + i*j + " ");
				j++;
			}
			j = 0;
			i++;
			System.out.println();
		}
	}

}
