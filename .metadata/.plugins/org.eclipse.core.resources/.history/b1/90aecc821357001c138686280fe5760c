package assign2;

import java.util.Scanner;

public class MainClass {

	public static void main(String[] args) {
		/*
		 *	편의점
		 * 지불 금액 :	3,210원
		 * 본인 금액 :	10,000원
		 * 거스름돈 -> ?
		 * 5000원 -> ?장
		 * 1000원 -> ?장
		 *  500원 -> ?개
		 *  100원 -> ?개
		 *  50원 -> ?개
		 *  10원 -> ?개
		 */
		Scanner scan = new Scanner(System.in);
		int price = scan.nextInt();
		int money = scan.nextInt();
		int pay_back = money - price;
		System.out.println("거스름돈 : " + pay_back + "원");
		System.out.println("5000원 : " + pay_back + "장");
		System.out.println("1000원 : " + pay_back + "장");
		System.out.println("500원 : " + pay_back + "개");
		System.out.println("100원 : " + pay_back + "개");
		System.out.println("50원 : " + pay_back + "개");
		System.out.println("10원 : " + pay_back + "개");
	}

}
