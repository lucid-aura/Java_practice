package whileLoop;

public class MainClass {

	public static void main(String[] args) {
		/*
		 * 초기화
		 * while(조건식) {
		 * 	처리
		 * 	연산식
		 * }
		 * 
		 * 초기화
		 * do {
		 * 	처리
		 * 	연산식
		 * }while(조건식);
		 */
		
		int w;
		w = 0;
		while( w < 10 ) {
			System.out.println("while loop" + w);
			w++;
		}
		
		int dw;
		dw = 0;
		do {
			System.out.println("while loop" + dw);
			dw++;
		}while( dw < 10 );
	}

}
